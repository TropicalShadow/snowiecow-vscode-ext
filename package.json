{
    "name": "snowiecow-buddy",
    "displayName": "Snowie Cow Buddy",
    "description": "Creates a snowie cow to code with you üêÆ",
    "publisher": "TropicalShadow",
    "icon": "media/middle_btn.webp",
    "version": "1.0.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/TropicalShadow/snowiecow-vscode-ext"
    },
    "engines": {
        "vscode": "^1.50.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:snowiecow-buddy.snowiecow"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "snowiecow-buddy.snowiecow",
                "title": "Snowie Cow Buddy"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "release-minor": "npm version minor"
    },
    "devDependencies": {
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.2.2",
        "@types/node": "14.x",
        "@types/vscode": "^1.50.0",
        "@typescript-eslint/eslint-plugin": "^4.26.0",
        "@typescript-eslint/parser": "^4.26.0",
        "eslint": "^7.27.0",
        "glob": "^7.1.7",
        "mocha": "^10.3.0",
        "typescript": "^4.3.2",
        "vscode-test": "^1.5.2"
    }
}
